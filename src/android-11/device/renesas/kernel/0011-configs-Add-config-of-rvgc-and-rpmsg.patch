Subject: [PATCH 11/27] configs: Add config of rvgc and rpmsg

---
 arch/arm64/configs/android_salvator_defconfig | 10 ++++++++--
 1 file changed, 8 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/configs/android_salvator_defconfig b/arch/arm64/configs/android_salvator_defconfig
index 78e1a897c64c..d95032ba74cd 100644
--- a/arch/arm64/configs/android_salvator_defconfig
+++ b/arch/arm64/configs/android_salvator_defconfig
@@ -79,6 +79,11 @@ CONFIG_CRYPTO_GHASH_ARM64_CE=y
 CONFIG_CRYPTO_AES_ARM64_CE_CCM=y
 CONFIG_CRYPTO_AES_ARM64_CE_BLK=y
 CONFIG_CRYPTO_AES_ARM64_NEON_BLK=y
+CONFIG_RCAR_MFIS=y
+CONFIG_DRM_RCAR_RVGC=m
+CONFIG_REMOTEPROC=y
+CONFIG_RCAR_CR7_REMOTEPROC=m
+CONFIG_RPMSG_VIRTIO=m
 CONFIG_KPROBES=y
 CONFIG_MODULES=y
 CONFIG_MODULE_UNLOAD=y
@@ -392,8 +397,9 @@ CONFIG_VIDEO_RENESAS_VSP1=y
 CONFIG_VIDEO_RENESAS_VSP_ALPHA_BIT_ARGB1555=0
 CONFIG_VIDEO_ADV748X=y
 CONFIG_DRM=y
-CONFIG_DRM_RCAR_DU=y
-CONFIG_DRM_RCAR_DW_HDMI=y
+# CONFIG_DRM_RCAR_DU is not set
+# CONFIG_DRM_RCAR_DW_HDMI is not set
+# CONFIG_DRM_RCAR_LVDS is not set
 CONFIG_DRM_PANEL_LVDS=y
 CONFIG_DRM_DUMB_VGA_DAC=y
 CONFIG_DRM_DW_HDMI_CEC=y
